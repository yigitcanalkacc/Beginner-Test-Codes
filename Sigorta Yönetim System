package Insurance;

import java.util.ArrayList;
import java.util.Date;

public class Main {

	public static void main(String[] args) {
		
		ArrayList<String> bireyselAdresler = new ArrayList<String>();
		bireyselAdresler.add("İstiklal Caddesi, İstanbul");
		
		ArrayList<String> kurumsalAdresler = new ArrayList<String>();
		kurumsalAdresler.add("Ataşehir, İstanbul");
		
		
		Bireysel bireyselhesaplar = new Bireysel("Ali", "Yılmaz", "ali.yilmaz@hotmail.com", "1234", "Öğretmen", 25, bireyselAdresler, new Date()); 
		bireyselhesaplar.HesapBilgi();
		
		System.out.println("----------------------------------------------------");
		
		Kurumsal kurumsalHesap = new Kurumsal("Ayşe", "Demir", "ayse.demir@gmail.com", "1234", "Avukat", 25, kurumsalAdresler, new Date());
		kurumsalHesap.HesapBilgi();
		
	}
	
}

package Insurance;

public interface Adres {

	public String getFullAdress();
	public String getAdressTipi();
	
	
}

